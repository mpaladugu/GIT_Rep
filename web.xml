    <!-- ============== "Public" Context Parameters ================= -->

  <!-- 
     actimize.ini.location is the ACM initialization file.
     You may use:
     - An absolute path - /my-directory/my-acm.ini
     - A relative path in the WAR - :/WEB-INF/config/my-acm.ini
     - A path with system variables:
       ${system:user.home} - process user home directory
       ${system:user.dir} - process running directory 
    -->
	<context-param>
		<param-name>actimize.ini.location</param-name>
		<param-value>C:\actimize\RCM_510\config</param-value>
	</context-param>	

