	<!-- These mappings are used for security reasons so that RCM prevents 
		details of its pages and internal logic from getting exposed.  -->	
	<!-- We treat 404 as 403 because do not want to expose that the page does 
		not exist and allow page enumeration. Therefore inform the user as if 
		the page does exist but they do no have access permissions -->	
	<!-- The error page message for 500 and an Exception is the same but in the
		case of an exception, we want to attempt to log the error - and (if in 
		development mode) display the stack trace on the page itself. -->		
    <error-page>
		<error-code>403</error-code>
		<location>/jsp/exception/error403.jsp</location>
	</error-page>
	<error-page>
		<error-code>404</error-code>
		<location>/jsp/exception/error403.jsp</location>
	</error-page>
	<error-page>
		<error-code>503</error-code>
		<location>/jsp/exception/error503.jsp</location>
	</error-page>
		<error-page>
		<error-code>500</error-code>
		<location>/jsp/exception/RuntimeError.jsp</location>
	</error-page>
	<error-page>
        <exception-type>java.lang.Throwable</exception-type>
        <location>/jsp/exception/RuntimeError.jsp</location>
    </error-page>
	
